<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aframe">

    <application
        android:name=".app.AFrameApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.laucher.LaucherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.material.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.material.ButtonNavigationActivity"
            android:label="@string/title_button_navigation"/>
        <activity
            android:name=".activity.material.MDLaucherActivity"
            android:label="@string/title_md_laucher"/>
        <activity
            android:name=".activity.toast.ToastActivity"
            android:label="@string/title_toast"/>
        <activity
            android:name=".activity.rxjava.RxJavaActivity"
            android:label="@string/title_rxjava_usage"/>
        <activity android:name=".calendar.CalendarActivity"
            android:label="@string/title_activity_calendar"
            android:theme="@style/AppTheme.NoActionBar"/>

    </application>

</manifest>